{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bitfinex",
  "title": "Bitfinex",
  "description": "",
  "icon": "command-icon.png",
  "author": "Yukai",
  "license": "MIT",
  "commands": [
    {
      "name": "funding-offers",
      "title": "Show Funding Offers",
      "subtitle": "Bitfinex",
      "description": "Show Funding Offer History on Bitfinex",
      "mode": "view"
    },
    {
      "name": "funding-interests",
      "title": "Show Funding Interests",
      "subtitle": "Bitfinex",
      "description": "Show Funding Interest History on Bitfinex",
      "mode": "view"
    },
    {
      "name": "lending-rates",
      "title": "Show Lending Rates",
      "subtitle": "Bitfinex",
      "description": "Show Lending Rate History on Bitfinex",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "title": "Bitfinex API Key",
      "name": "api_key",
      "required": true,
      "type": "password",
      "description": "Bitfinex API Key"
    },
    {
      "title": "Bitfinex API Secret",
      "name": "api_secret",
      "required": true,
      "type": "password",
      "description": "Bitfinex API Secret"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.37.0",
    "bfx-api-node-models": "^1.6.3",
    "bitfinex-api-node": "^5.0.4",
    "node-fetch": "^3.2.6",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
